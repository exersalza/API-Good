CREATE TABLE `roll_text` (
	`ID` INT(11) NOT NULL AUTO_INCREMENT,
	`Name` VARCHAR(50) NULL DEFAULT NULL COLLATE 'utf8mb4_general_ci',
	`Text` VARCHAR(50) NULL DEFAULT NULL COLLATE 'utf8mb4_general_ci',
	PRIMARY KEY (`ID`) USING BTREE,
	INDEX `FK1_NAME_TOKENS` (`Name`) USING BTREE,
	CONSTRAINT `FK1_NAME_TOKENS` FOREIGN KEY (`Name`) REFERENCES `dcbot`.`tokens` (`Name`) ON UPDATE NO ACTION ON DELETE NO ACTION
)
COLLATE='utf8mb4_general_ci'
ENGINE=InnoDB
AUTO_INCREMENT=3
;
