## [SQL SHIT] ##

HOST = 'localhost'
PASSWORD = ''
DB = 'dcbot'
USER = 'root'

db = mysql.connector.connect(
        host=HOST,
        user=USER,
        password=PASSWORD,
        database=DB
)

CUR = db.cursor(buffered=True)

## [BOT SHIT] ##

config_shit = []
foo = 1

while foo != 3:
    foo += 1
    CUR.execute("SELECT * FROM tokens WHERE Name='[NAME]'")
    config_shit.append(CUR.fetchone()[foo])

TOKEN = config_shit[0]
PREFIX = config_shit[1]
ESCAPE = ''
FLASK = False
